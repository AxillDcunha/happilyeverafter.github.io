<div class="website-blogs-page-all">
  <div class="website-blogs-page-all-sub">
    <div class="container">
      <div class="row">
        <% vendor.forEach(vendor => { %> <% if (vendor.type==="photographer") {
        %>

        <div class="col-md-4">
          <div class="blog-card blog-card-blog">
            <div class="blog-card-image">
              <a href="/<%=vendor._id%>?blog=blog">
                <img class="" src="<%=vendor.img%>" />
              </a>
              <div class="ripple-cont"></div>
            </div>
            <div class="blog-table">
              <h6 class="blog-category blog-text-success">
                <i class="far fa-newspaper"></i> News
              </h6>
              <h4 class="blog-card-caption">
                <a href="/<%=vendor._id%>?blog=blog"
                  >Lorem Ipsum is simply dummy text of the printing and</a
                >
              </h4>
              <p class="blog-card-description">
                <%= vendor.description.slice(0,99) %>...
              </p>
              <div class="ftr">
                <div class="author">
                  <a href="/<%=vendor._id%>?blog=blog">
                    <img
                      src="https://picsum.photos/id/1005/5760/3840"
                      alt="..."
                      class="avatar img-raised"
                    />
                    <span><%=vendor.name %></span>
                  </a>
                </div>
                <div class="stats"><i class="far fa-clock"></i> 10 min</div>
              </div>
            </div>
          </div>
        </div>

        <% } %> <% }) %>
      </div>
    </div>
  </div>
</div>
